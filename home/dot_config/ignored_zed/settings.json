{
  "base_keymap": "VSCode",
  "buffer_font_family": "JetBrainsMono Nerd Font Mono",
  "buffer_font_features": { "calt": true, "zero": true, "cv14": true },
  "buffer_font_size": 17.0,
  "current_line_highlight": "none",
  "cursor_blink": false,
  "format_on_save": "on",
  "git": { "inline_blame": { "enabled": false } },
  "icon_theme": "Material Icon Theme",
  "languages": {
    "Haskell": { "tab_size": 2 },
    "Racket": {
      "formatter": { "external": { "command": "raco", "arguments": ["fmt"] } }
    },
    "SQL": { "formatter": { "external": { "command": "format-postgres" } } },
    "Typst": { "formatter": { "external": { "command": "typstyle" } } }
  },
  "lsp": {
    "golangci-lint": {
      "initialization_options": {
        "command": [
          "golangci-lint",
          "run",
          "--show-stats=false",
          "--output.json.path=stdout"
        ]
      }
    },
    "gopls": { "initialization_options": { "formatting.gofumpt": true } },
    "rust-analyzer": {
      "initialization_options": { "rustfmt": { "extraArgs": ["+nightly"] } }
    }
  },
  "project_panel": { "auto_fold_dirs": false, "dock": "right" },
  "show_edit_predictions": false,
  "tab_bar": { "show_nav_history_buttons": false },
  "telemetry": { "diagnostics": false, "metrics": false },
  "theme": {
    "dark": "Catppuccin Macchiato",
    "light": "Catppuccin Latte",
    "mode": "system"
  },
  "ui_font_family": "Wix Madefor Display",
  "vim": {
    "toggle_relative_line_numbers": true,
    "use_system_clipboard": "on_yank"
  },
  "vim_mode": true
}
