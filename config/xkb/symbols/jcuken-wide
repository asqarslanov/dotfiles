    key <AD08> { [      Cyrillic_ghe,      Cyrillic_GHE  ] };
    key <AD09> { [      Cyrillic_sha,      Cyrillic_SHA  ] };
    key <AD10> { [    Cyrillic_shcha,    Cyrillic_SHCHA  ] };
    key <AD11> { [       Cyrillic_ze,       Cyrillic_ZE  ] };
    key <AD12> { [        Cyrillic_e,        Cyrillic_E  ] };

    key <AC01> { [       Cyrillic_ef,       Cyrillic_EF  ] };
    key <AC02> { [     Cyrillic_yeru,     Cyrillic_YERU  ] };
