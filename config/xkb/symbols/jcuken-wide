    key <AC10> { [       Cyrillic_de,       Cyrillic_DE  ] };
    key <AC11> { [      Cyrillic_zhe,      Cyrillic_ZHE  ] };

    key <AB01> { [       Cyrillic_ya,       Cyrillic_YA  ] };
